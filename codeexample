import * packages;

public class CodeExample
{
  boolean decide = false;
  int integer = 0;
  char character = new char;
  random randomNumber = new random;
  
  
  public static void main (string [] args)
  {
    for (int i == 0; i < 200; ++i)
    {
      integer = i;
      
      if (integer >= 100)
      {
        decide = true;
        return decide;
      }
      else
      {
        integer = integer + random;
      }
    }
  }
}
